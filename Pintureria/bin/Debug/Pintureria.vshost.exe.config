<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="IVA" type="System.Configuration.DictionarySectionHandler"/>
  </configSections>
  <connectionStrings>
    <add name="nico" connectionString="Data Source=(local);Initial Catalog=Pintureria;Integrated Security=True"/>
    <add name="gera" connectionString="Data Source=GERA-PC\SQLEXPRESS;Initial Catalog=Pintureria;Integrated Security=True"/>
    <add name="Pintureria.Properties.Settings.PintureriaConnectionStringReportes" connectionString="Data Source=(local);Initial Catalog=Pintureria;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <IVA>
    <add key="1" value="21"/>
  </IVA>
 
  <appSettings>
    <!-- Valor por defecto del combo provincia segun el id de la provincia -->
    <add key="Provincia" value="1"/>
    <!-- Valor por defecto del combo Localidad verificar bien el id de la localidad -->
    <add key="Localidad" value="4"/>
  </appSettings>

  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>

